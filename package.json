    "drizzle-orm": "^0.38.2",
    "fuzzysort": "^3.0.2",
    "jsdom": "^23.2.0",
    "node-cache": "^5.1.2",
    "numeral": "^2.0.6",
    "rimraf": "^6.0.1",
    "turndown": "^7.1.2",
    "youtube-dl-exec": "^3.0.6",
    "zod": "^3.22.4"
  },
  "devDependencies": {
